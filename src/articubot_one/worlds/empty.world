<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    
    <physics name= "default_physics" default="1" type="ode">
      <ode>
      	<real_time_update_rate>1000.0</real_time_update_rate>
      </ode>
    </physics> 
  </world>
</sdf>
